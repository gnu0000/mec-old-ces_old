:query update.bin
//
// This is the metadata for the update SQL.
//
// Multiple strings for the same window ID
// are concatenated together at this time
//
// USHORT             char []
// Window ID          Query String
//-------------------------------------------------------------------------
JobD,            "UPDATE Job "
JobD,            "SET JobID = @JobID, "
JobD,            "Description = @Description, "
JobD,            "Location = @Location, "
JobD,            "PrimaryCountyKey = @"DID_COUNTYCAT"CountyKey, "
JobD,            "District = @"DID_DISTRICT"CodeValueKey, "
JobD,            "DavisBaconWages = @DavisBaconWages, "
JobD,            "LettingMonth = @LettingMonth, "
JobD,            "WorkType = @"DID_WORKTYPE"CodeValueKey, "
JobD,            "ConstructionEngineeringPct = @ConstructionEngineeringPct, "
JobD,            "ContingencyPercent = @ContingencyPercent, "
JobD,            "WorkingDayCalendarKey = @"DID_WORKDAYCAL"WorkingDayCalendarKey, "
JobD,            "Estimator = @"DID_ESTIMATOR"UserKey, "
JobD,            "DateChecked = @DateChecked, "
JobD,            "DateApproved = @DateApproved, "
JobD,            "CheckedBy = @"DID_CHECKEDBY"UserKey, "
JobD,            "ApprovedBy = @"DID_APPROVEDBY"UserKey "
JobD,            ",UnitType = @CURUNITSYS "
JobD,            "WHERE JobKey = %JobKey "

UpdateBaseDate,  "UPDATE Job "
UpdateBaseDate,  "SET BaseDate = @MAXBASEDATE "
UpdateBaseDate,  "WHERE JobKey = %JobKey "

JobAltGroupD,    "/* bamsces.met */ "
JobAltGroupD,    "UPDATE JobAlternateGroup "
JobAltGroupD,    "SET JobAlternateGroupID = @JobAlternateGroupID, "
JobAltGroupD,    "Description = @Description "
JobAltGroupD,    "WHERE JobKey = %JobKey "
JobAltGroupD,    "AND JobAlternateGroupKey = %JobAlternateGroupKey "

JobAltD,         "/* bamsces.met */ "
JobAltD,         "UPDATE JobAlternate "
JobAltD,         "SET JobAlternateID = @JobAlternateID, "
JobAltD,         "Description = @Description "
JobAltD,         "WHERE JobKey = %JobKey "
JobAltD,         "AND JobAlternateGroupKey = %JobAlternateGroupKey "
JobAltD,         "AND JobAlternateKey = %JobAlternateKey "

JobBreakdownD,   "UPDATE JobBreakdown "
JobBreakdownD,   "SET JobBreakdownID = @JobBreakdownID, "
JobBreakdownD,   "Description = @Description, "
JobBreakdownD,   "PavementWidth = @PavementWidth, "
JobBreakdownD,   "Length = @Length, "
JobBreakdownD,   "PavementDepth = @PavementDepth, "
JobBreakdownD,   "RoadType = @"DID_ROADTYPE"CodeValueKey, "
JobBreakdownD,   "StructureType = @"DID_STRUCTTYPE"CodeValueKey, "
JobBreakdownD,   "StructureID = @StructureID, "
JobBreakdownD,   "StructureLength = @StructureLength, "
JobBreakdownD,   "StructureWidth = @StructureWidth, "
JobBreakdownD,   "WorkType = @"DID_WORKTYPE"CodeValueKey, "
JobBreakdownD,   "DetailedEstimate = @DetailedEstimate "
JobBreakdownD,   "WHERE JobKey = %JobKey "
JobBreakdownD,   "AND JobBreakdownKey = %JobBreakdownKey "
//  
JobBreakdownD,   "/* REVISED MetaData */ " 
JobBreakdownD,   " BEGIN"
JobBreakdownD,       " UPDATE Job "
JobBreakdownD,       " SET EstimatedCost = 0.0 "
JobBreakdownD,             " WHERE JobKey = %JobKey "
JobBreakdownD,       " END "
//
JobBreakdownD,   "IF (NULL != (SELECT SUM(PreliminaryEstimateCost) "
JobBreakdownD,              " FROM JobBreakdown"
JobBreakdownD,              " WHERE JobKey = %JobKey"
JobBreakdownD,              " AND DetailedEstimate = 0 )) "
JobBreakdownD,       " BEGIN"
JobBreakdownD,           " UPDATE Job "
JobBreakdownD,           " SET EstimatedCost = "
JobBreakdownD,                 " (SELECT SUM(PreliminaryEstimateCost) "
JobBreakdownD,                 " FROM JobBreakdown"
JobBreakdownD,                 " WHERE JobKey = %JobKey"
JobBreakdownD,                 " AND DetailedEstimate = 0 ) "
JobBreakdownD,           " WHERE JobKey = %JobKey "
JobBreakdownD,       " END "
//
JobBreakdownD,   "IF (NULL != (SELECT SUM(DetailedEstimateCost) "
JobBreakdownD,              " FROM JobBreakdown"
JobBreakdownD,              " WHERE JobKey = %JobKey"
JobBreakdownD,              " AND DetailedEstimate = 1 )) "
JobBreakdownD,       " BEGIN"
JobBreakdownD,           " UPDATE Job "
JobBreakdownD,           " SET EstimatedCost = EstimatedCost + "
JobBreakdownD,                 " (SELECT SUM(DetailedEstimateCost) "
JobBreakdownD,                 "  FROM JobBreakdown"
JobBreakdownD,                 "  WHERE JobKey = %JobKey"
JobBreakdownD,                 "  AND DetailedEstimate = 1 ) "
JobBreakdownD,           " WHERE JobKey = %JobKey "
JobBreakdownD,       " END "
//
JobBreakdownD,   " UPDATE Job "
JobBreakdownD,   " SET EstimateTotal = "
JobBreakdownD,   " (EstimatedCost * (1.00 "
JobBreakdownD,   " + ConstructionEngineeringPct "
JobBreakdownD,   " + ContingencyPercent) ) "
JobBreakdownD,   " WHERE JobKey = %JobKey "


ChJobUserD,      "UPDATE JobUser "
ChJobUserD,      "SET UserID = @UserID, "
ChJobUserD,      "AccessPrivilege = @"DID_ACCESSPRIV"AccessPrivilege "
ChJobUserD,      "WHERE JobKey = %JobKey "
ChJobUserD,      "AND UserID = '%UserID' "

JobItemD,        "/* JobItemD */ "
JobItemD,        "UPDATE JobItem "
JobItemD,        "SET "
JobItemD,        "StandardItemKey=@"StandardItemCatL"StandardItemKey, "
JobItemD,        "JobAlternateGroupKey=@"DID_ALT_GROUP"JobAlternateGroupKey, "
JobItemD,        "JobAlternateKey=@"DID_ALT"JobAlternateKey, "
JobItemD,        "Quantity=@Quantity, "
JobItemD,        "UnitPrice=@UnitPrice, "
